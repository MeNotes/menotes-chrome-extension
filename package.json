{
  "name": "keepario",
  "version": "0.0.1",
  "description": "Keep your meeting notes",
  "scripts": {
    "build": "export NODE_ENV=production && node utils/build.js",
    "start": "export NODE_ENV=development && node utils/webserver.js",
    "serve": "PORT=6002 npm start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "eslint . --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "6.1.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "file-loader": "6.2.0",
    "fs-extra": "10.0.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "husky": "^4.3.8",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "style-loader": "3.1.0",
    "webpack": "5.45.1",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "simplemde": "^1.11.2",
    "storeon": "^3.1.4"
  }
}
